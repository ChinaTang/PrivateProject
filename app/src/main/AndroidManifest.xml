<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.di.tang.privateproject">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.di.tang.firstboundary.activity.MainInterfaceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.di.tang.gateway.GateWayActivity">
            <intent-filter>
                <action android:name="tang.di.action.MainInterfaceActivity"/>

                <category android:name="tang.di.category.MainInterfaceActivity"/>
            </intent-filter>
        </activity>
        <activity android:name="com.di.tang.seconddetail.activity.DetailActivity">
            <intent-filter>
                <action android:name="tang.di.action.DetailActivity"/>

                <action android:name="tang.di.category.DetailActivity"/>
            </intent-filter>
        </activity>
    </application>

</manifest>